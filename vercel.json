{
    "version": 2,
    "builds": [
        {
            "src": "static/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "chic_images/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "installCommand": "bash vercel-build.sh"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "/media/$1"
        },
        {
            "src": "/(.*)",
            "dest": "chic_images/wsgi.py"
        }
    ]
}